GITLAB_TOKEN=your_gitlab_personal_access_token
GITLAB_API=https://gitlab.com/api/v4
GITLAB_DEFAULT_PROJECT=RD_soft/simpliciti-frontend/geored-v3
MAX_DIFF_CHARS=50000

# LLM Provider Configuration
# Supported providers: openrouter, openai, ollama, azure
LLM_PROVIDER=openrouter

# API Key (not required for Ollama)
LLM_API_KEY=your_api_key_here

# Model to use
LLM_MODEL=anthropic/claude-3.5-sonnet

# Custom API URL (optional - overrides provider defaults)
# LLM_API_URL=http://localhost:11434/v1/chat/completions

# ===== Provider-specific examples =====

# OpenRouter (default)
# LLM_PROVIDER=openrouter
# LLM_API_KEY=sk-or-v1-...
# LLM_MODEL=anthropic/claude-3.5-sonnet

# OpenAI
# LLM_PROVIDER=openai
# LLM_API_KEY=sk-...
# LLM_MODEL=gpt-4o

# Ollama (local)
# LLM_PROVIDER=ollama
# LLM_MODEL=llama3.1:8b
# LLM_API_URL=http://localhost:11434/v1/chat/completions

# Azure OpenAI
# LLM_PROVIDER=azure
# LLM_API_KEY=your_azure_key
# LLM_MODEL=gpt-4
# LLM_API_URL=https://your-resource.openai.azure.com/openai/deployments/your-deployment/chat/completions?api-version=2024-02-15-preview

# ===== Legacy variables (still supported) =====
# OPENROUTER_API_KEY=sk-or-v1-...
# OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
